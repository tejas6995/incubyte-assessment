{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "interface",
      "(?=content|api)",
      "context/",
      "mock/",
      "config",
      "utils/",
      "hooks/",
      "(components/|./index)",
      ".svg",
      "^../(.*)$",
      "(?=./styles.module.scss)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  